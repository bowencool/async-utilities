{
  "engines": {
    "node": ">= 14.15.5",
    "npm": ">= 6.14.0"
  },
  "scripts": {
    "dev": "NODE_ENV=development vitepress dev example",
    "build": "NODE_ENV=production rollup -c",
    "prebuild": "rm -rf cjs es",
    "build:docs": "NODE_ENV=production vitepress build example",
    "prebuild:docs": "patch-package && rm -rf docs",
    "postbuild:docs": "mv example/.vitepress/dist/ docs/",
    "prepublishOnly": "npm run build",
    "postpublish": "git push && git push --tags",
    "version": "npm run build:docs && conventional-changelog -p angular -i CHANGELOG.md -s && git add .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "files": [
    "src",
    "es",
    "cjs"
  ],
  "dependencies": {
    "core-js": "^3.17.3"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@vitejs/plugin-vue-jsx": "^1.1.8",
    "@vue/compiler-sfc": "^3.2.11",
    "conventional-changelog-cli": "^2.1.1",
    "epic-spinners": "^1.1.0",
    "fs-extra": "^10.0.0",
    "markdown-it-checkbox": "^1.1.0",
    "patch-package": "^6.4.7",
    "rollup": "^2.56.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "typescript": "^4.4.3",
    "vitepress": "^0.17.3",
    "vue": "^3.2.11"
  },
  "entry": "src/index.ts",
  "main": "cjs/index.js",
  "module": "es/index.js",
  "common": "cjs/index.js",
  "types": "es/index.d.ts",
  "license": "MIT",
  "homepage": "https://github.com/bowencool/async-utils",
  "keywords": [
    "typescript",
    "async",
    "promise",
    "utils",
    "debounce",
    "throttle",
    "concurrent",
    "abort",
    "cancel",
    "timeout"
  ],
  "description": "async utils",
  "author": "bowencool",
  "name": "bowen-async-utils",
  "version": "0.6.2"
}
